<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Méthode de distribution -->
    <key>method</key>
    <string>app-store</string>
    
    <!-- Configuration de signature -->
    <key>signingStyle</key>
    <string>automatic</string>
    
    <!-- Team ID (à configurer avec votre équipe Apple Developer) -->
    <!-- <key>teamID</key>
    <string>VOTRE_TEAM_ID</string> -->
    
    <!-- Options de compilation -->
    <key>uploadBitcode</key>
    <false/>
    
    <key>uploadSymbols</key>
    <true/>
    
    <key>compileBitcode</key>
    <false/>
    
    <!-- Gestion des provisioning profiles -->
    <key>manageAppVersionAndBuildNumber</key>
    <false/>
    
    <!-- Destinations de distribution -->
    <key>destination</key>
    <string>upload</string>
    
    <!-- Options pour TestFlight -->
    <key>testFlightInternalTestingOnly</key>
    <false/>
    
    <!-- Manifeste pour App Thinning -->
    <key>manifest</key>
    <dict>
        <key>appURL</key>
        <string>https://new.dinorapp.com/app/dinor-app.ipa</string>
        <key>displayImageURL</key>
        <string>https://new.dinorapp.com/app/icon-57.png</string>
        <key>fullSizeImageURL</key>
        <string>https://new.dinorapp.com/app/icon-512.png</string>
    </dict>
    
    <!-- Provisioning profiles spécifiques (optionnel) -->
    <!-- 
    <key>provisioningProfiles</key>
    <dict>
        <key>com.dinorapp.mobile</key>
        <string>VOTRE_PROVISIONING_PROFILE_UUID</string>
    </dict>
    -->
    
    <!-- Configuration Xcode Cloud (optionnel) -->
    <key>generateAppStoreInformation</key>
    <true/>
</dict>
</plist>